@prefix ex: <https://www.bgee.org/sparql/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

ex:001 a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Total existing entities which have an ontology property"@en ;
    sh:prefixes _:sparql_examples_prefixes ;
    sh:select """
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
SELECT ?nSNOMED ?nDOID ?nMondo ?nOrdo 
WHERE {
  { SELECT (COUNT(DISTINCT ?itemS) AS ?nSNOMED) WHERE {
      ?itemS wdt:P5806 ?snomedId .
    }
  }
  { SELECT (COUNT(DISTINCT ?itemD) AS ?nDOID) WHERE {
      ?itemD wdt:P699 ?doidId .
    }
  }
  { SELECT (COUNT(DISTINCT ?itemD) AS ?nMondo) WHERE {
      ?itemD wdt:P5270 ?MondoId .
    }
  }
    { SELECT (COUNT(DISTINCT ?itemD) AS ?nOrdo) WHERE {
      ?itemD wdt:P1550 ?ordoId .
    }
  }
  
}
""" ;
    schema:target <https://qlever.dev/wikidata>, <https://query.wikidata.org/>  .
